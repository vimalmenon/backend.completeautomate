[flake8]
max-line-length = 88
extend-ignore = E203, W503, E501
exclude =
    .git,
    __pycache__,
    .venv,
    .tox,
    build,
    dist,
    *.egg-info,
    .mypy_cache,
    .pytest_cache,
    .eggs,
    *.egg
max-complexity = 10
per-file-ignores =
    __init__.py:F401,F403
    tests/*:S101,S106
